<% provide(:title, "#{@student.first_name} #{@student.last_name}") %>

<%= render partial: "shared/profile", locals: { user: @student, id: params[:id], reviews: @reviews, request_received: @request_received, request_accepted: @request_accepted, type: "student" } %>

<script>
  $(".star-rating").raty({
    path: "/assets",
    readOnly: true,
    score: function() {
      return $(this).attr("data-score");
    }
  });
</script>